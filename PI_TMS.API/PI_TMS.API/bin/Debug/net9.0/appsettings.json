{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=${ConnectionStrings__DefaultConnection};TrustServerCertificate=True;"
  },
  "Jwt": {
    "Key": "${Jwt__Key}",
    "Issuer": "${Jwt__Issuer}",
    "Audience": "${Jwt__Audience}",
    "ExpiresInMinutes": "${Jwt__ExpiresInMinutes}"
  }
}
